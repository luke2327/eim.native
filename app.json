{
  "expo": {
    "name": "eim.frontend",
    "slug": "eim",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "packagerOpts": {
      "sourceExts": ["jsx", "js"]
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/test_icon.png",
    "splash": {
      "image": "./assets/test_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
